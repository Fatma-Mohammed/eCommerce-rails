<%= paginator.render do -%>
      <div class="col-md-2"><%= first_page_tag unless current_page.first? %></div>
        <div class="col-md-7">
        <ul>
          <% each_page do |page| -%>
            <% if page.left_outer? || page.right_outer? || page.inside_window? -%>
              <%= page_tag page %>
            <% elsif !page.was_truncated? -%>
              <%= gap_tag %>
            <% end -%>
          <% end -%>
        </ul>
      </div>
      <div class="col-md-2">
        <%= last_page_tag unless current_page.last? %>
      </div>
<% end -%>